  <!--
  The set of hex-character headers, in Regex form, to detect file types.

  Many of these are sourced from
  https://onedrive.visualstudio.com/OneDrive%20Service//_git/OneDrive.Storage?path=%2Fserver%2Fstorage%2Fprivate%2FDatabase%2FSetup%2Fdefault%2FSettings%2FMimeType.xml

  While OneDrive doesn't use a "sniffer" anymore as of 3/9/2018, we need to in some cases because we don't
  know the file name or the MIME type -->
  <Hex-Headers>
    <!-- Images -->
    <HexHeader header-bytes="424D" content-type="image/bmp" />
    <HexHeader header-bytes="FFD8FF" content-type="image/jpeg" />
    <HexHeader header-bytes="474946383761" content-type="image/gif" />
    <HexHeader header-bytes="474946383961" content-type="image/gif" />
    <HexHeader header-bytes="492049" content-type="image/tiff" />
    <HexHeader header-bytes="49492A00" content-type="image/tiff" />
    <HexHeader header-bytes="4D4D002A" content-type="image/tiff" />
    <HexHeader header-bytes="4D4D002B" content-type="image/tiff" />
    <HexHeader header-bytes="89504E470D0A1A0A" content-type="image/png" />
    <HexHeader header-bytes="00000100" content-type="image/vnd.microsoft-icon" />
    <HexHeader header-bytes="4949BC" content-type="image/vnd.ms-photo" />
    <!-- https://developers.google.com/speed/webp/docs/riff_container#webp_file_header -->
    <!-- "RIFF" {4-byte size} "WEBP" -->
    <HexHeader header-bytes="52494646........57454250" content-type="image/webp" />

    <!-- Videos -->
    <!-- Sourced from https://www.garykessler.net/library/file_sigs.html
         and http://en.wikipedia.org/wiki/List_of_file_signatures -->
    <HexHeader header-bytes="000001B" content-type="video/mpeg"/>
    <!-- Note: mkv header could also be a webm video - we assume mkv is more common -->
    <HexHeader header-bytes="1A45DFA3" content-type="video/x-matroska" />
    <HexHeader header-bytes="3026B2758E66CF11" content-type="video/x-ms-wmv" />
    <HexHeader header-bytes="A6D900AA0062CE6C" content-type="video/x-ms-wmv" />
    <HexHeader header-bytes="464C5601" content-type="video/x-flv"/>
    <!-- "RIFF" {4-byte size} "AVI LIST" -->
    <HexHeader header-bytes="52494646........415649204C495354" content-type="video/avi"/>

    <HexHeader header-bytes="........667479704D534E56" content-type="video/mp4"/>
    <HexHeader header-bytes="........6674797069736F6D" content-type="video/mp4"/>
    <HexHeader header-bytes="........667479706D703432" content-type="video/mp4"/>
    <HexHeader header-bytes="........6674797071742020" content-type="video/quicktime"/>
